{
  "name": "cmq-utils",
  "version": "0.0.1",
  "description": "utils",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.global.js",
  "scripts": {
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "predev": "node scripts/generator.js",
    "prebuild": "node scripts/generator.js",
    "dev": "tsc --noEmit && rollup -cw --environment NODE_ENV:development",
    "build": "tsc --noEmit && rollup -c --environment NODE_ENV:production && npm run types",
    "prepublishOnly": "npm run patch && node scripts/generator.js",
    "types": "tsc --emitDeclarationOnly"
  },
  "keywords": [
    "cmq-utils",
    "utils",
    "cmq"
  ],
  "files": [
    "lib"
  ],
  "author": "chenmq",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "json-templater": "^1.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@babel/runtime-corejs3": "^7.18.9"
  },
  "lint-staged": {
    "*.ts": [
      "tsc --noEmit",
      "eslint --fix",
      "prettier --write"
    ]
  }
}
